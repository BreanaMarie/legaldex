<div class="container">
  <div class="row">
    <div class="col-sm-4 profile_pic">
      <img src="<%= @user.image%>">
      <br> 
      <h4><%= "#{@user.first_name} #{@user.last_name}"  %> </h4>
      <br>
      <p><%= "Joined on #{@user.created_at.to_date.to_formatted_s(:long_ordinal)}" %></p>
      <br>
    </div>
  </div>
</div>

